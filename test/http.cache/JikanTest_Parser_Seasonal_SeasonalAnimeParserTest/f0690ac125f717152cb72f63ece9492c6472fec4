      <span style="display: none;" class="js-members">410</span>
      <span style="display: none;" class="js-score">0</span>
      <span style="display: none;" class="js-start_date">20180106</span>
      <span style="display: none;" class="js-title">Pikachin-Kit</span>
    </div>

    <div class="prodsrc">
      <div class="video"><a href="https://myanimelist.net/anime/36552/Pikachin-Kit/video" class="ga-click" title="Watch Promotional Video"><i class="malicon malicon-movie-pv"></i></a></div>
